@{
    ViewData["Title"] = "Home Page";
}

<div id="carCarousel" class="carousel slide" data-bs-ride="carousel">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="~/Image/hondacivic.png" class="d-block w-100" style="height: 100vh; object-fit: cover;" alt="BMW 1" />
        </div>
        <div class="carousel-item">
            <img src="~/Image/toyotavios.png" class="d-block w-100" style="height: 100vh; object-fit: cover;" alt="BMW 2" />
        </div>
        <div class="carousel-item">
            <img src="~/Image/toyotawigo.png" class="d-block w-100" style="height: 100vh; object-fit: cover;" alt="BMW 3" />
        </div>
        <div class="carousel-item">
            <img src="~/Image/hondahr-v.png" class="d-block w-100" style="height: 100vh; object-fit: cover;" alt="BMW 4" />
        </div>
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#carCarousel" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carCarousel" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
    </button>
</div>